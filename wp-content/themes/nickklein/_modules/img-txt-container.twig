<section class="img-txt-container section-container">
    <h2 class="animate fade-in-quick">{{content.section_title}}</h2>
    <div class="container">
        <div class="row">
            <div class="image col-xl-4 col-md-5 wpjs slide-left">
                <img src="{{TimberImage(content.image.ID).src|resize(260,320)}}" alt="{{TimberImage(content.image.ID).alt}}">
            </div>
            <div class="introduction col-xl-8 col-md-6 wpjs slide-right">
                {{content.content_txt}}
            </div>
        </div>
    </div>
</section>
